        "zendframework/zend-developer-tools": "dev-master",
        "doctrine/doctrine-orm-module": "dev-master",
        "symfony/yaml": "^2.7",
        "bjyoungblood/BjyProfiler": "^1.1",
        "zendframework/zend-console": "^2.5",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/doctrine-mongo-odm-module": "^0.9.1",
        "sonata-project/doctrine-extensions": "^1.0",
        "ruby-project/ruby-admin": "dev-master"

    },
    "autoload": {
        "psr-4": {
            "Application\\Entity\\": "module/Application/src/Application/Entity/",
            "Application\\Model\\": "module/Application/src/Application/Model/"
