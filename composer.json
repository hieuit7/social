{
    "name": "hieuit7/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "hieuit7",
        "skeleton"
    ],
    "repositories": [
        {
            "type": "vcs",
